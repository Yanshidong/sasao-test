<?xml version="1.0" encoding="UTF-8"?> <!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd"> <!-- 配置生成器 -->
<generatorConfiguration>
    <!--执行generator插件生成文件的命令： call mvn mybatis-generator:generate -e -->
    <!-- 引入配置文件 -->
    <properties
            resource="config/mybatis-generator/mybatisGeneratorinit.properties"/> <!--classPathEntry:数据库的JDBC驱动,换成你自己的驱动位置 可选 --> <!--<classPathEntry location="D:\generator_mybatis\mysql-connector-java-5.1.24-bin.jar" /> --> <!-- 一个数据库一个context --> <!--defaultModelType="flat" 大数据字段，不分表 -->

    <context id="Mysql">
       <!--据说这是个分页插件-->
       <plugin type="org.mybatis.generator.plugins.RowBoundsPlugin"/>
       <!--生成的POJO实现java.io.Serializable接口-->
       <plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
       <!--重写实体类的toString方法-->
       <plugin type="org.mybatis.generator.plugins.ToStringPlugin" />
        <!--重写实体 hashCode 和 equals(比较两个实体的所有字段是否相等) 方法-->
       <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin"/>
        <!--不知道干嘛用的-->
       <plugin type="org.mybatis.generator.plugins.SqlMapConfigPlugin"   />
       <!--mapper.xml 中 生成 cache标签,并给标签配置属性-->
       <plugin type="org.mybatis.generator.plugins.CachePlugin">
           <property name="cache_eviction" value="LRU" />
           <property name="cache_flushInterval" value="60000" />
           <property name="cache_readOnly" value="true" />
           <property name="cache_size" value="1560" />
       </plugin>

       <!--重命名生成的 **Example ,比如UserExample=>UserQuery-->
       <plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin">
           <property name="searchString" value="Example$"/>
           <property name="replaceString" value="Query"/>
       </plugin>
       
       <!--添加注释-->
       <commentGenerator>
           <property name="addRemarkComments" value="true"/>
            <property name="suppressDate" value="true"/>
       </commentGenerator>

       <!--jdbc连接信息-->
       <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                       connectionURL="jdbc:mysql://127.0.0.1:3306/wd7"
                       userId="root"
                       password="baicai@930715"/>
       <!--生成实体类目录-->
       <javaModelGenerator targetPackage="com.cjs.example.entity" targetProject="src/main/java"/>
        <!--生成 mapper.xml 目录-->
        <sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources"/>
        <!--生成 mapper.xml对应的 dao-->
       <javaClientGenerator type="XMLMAPPER" targetPackage="com.cjs.example.dao" targetProject="src/main/java"/>
        <!-- tableName="%" 来为所有表生成model -->
       <table tableName="%">
           <!--生成实体前缀替换，-->
           <domainObjectRenamingRule searchString="^Tbl" replaceString=""/>
       </table>
   </context>
</generatorConfiguration>
